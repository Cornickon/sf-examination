<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Current_date_checkbox__c</fullName>
    <description>Current date checkbox in Exam object</description>
    <externalId>false</externalId>
    <formula>AND(TODAY()&gt;= Exam_Start_Date__c, TODAY()&lt;= Exam_End_Date__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Current date within exam dates</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
